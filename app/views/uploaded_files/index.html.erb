	<div class="page-header">
	  <%= link_to new_uploaded_file_path, class: 'btn btn-primary' do %>
	    <span class="glyphicon glyphicon-plus"></span>
	    New Uploaded file
	  <% end %>
	  <h1>Listing uploaded_files</h1>
	</div>

	<div class="table-responsive">
	  <table class="table table-striped table-bordered table-hover">
	    <thead>
	      <tr>
	            <th>Audio</th>
	            <th></th>
	        <th></th>
	        <th></th>
	      </tr>
	    </thead>

	    <tbody>
	      <%= content_tag_for(:tr, @uploaded_files) do |uploaded_file| %>
	            <td><%= uploaded_file.audio.original_filename %></td>
	            <td><%= link_to 'Show', uploaded_file %></td>
	        <td><%= link_to 'Edit', edit_uploaded_file_path(uploaded_file) %></td>
	        <td><%= link_to 'Destroy', uploaded_file, method: :delete, data: { confirm: 'Are you sure?' } %></td>
	      <% end %>
	    </tbody>
	  </table>
	</div>
